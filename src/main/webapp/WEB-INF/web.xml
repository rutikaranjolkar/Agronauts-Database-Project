<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>edu.neu.agronauts.ui.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>edu.neu.agronauts.ui.servlets.HomeServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>SaveFarmerRequestServlet</display-name>
		<servlet-name>SaveFarmerRequestServlet</servlet-name>
		<servlet-class>edu.neu.agronauts.ui.servlets.SaveFarmerRequestServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ApproveServiceRequestServlet</display-name>
		<servlet-name>ApproveServiceRequestServlet</servlet-name>
		<servlet-class>edu.neu.agronauts.ui.servlets.ApproveServiceRequestServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>FarmerRegisterServlet</display-name>
		<servlet-name>FarmerRegisterServlet</servlet-name>
		<servlet-class>edu.neu.agronauts.ui.servlets.FarmerRegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>FarmerRequestListServlet</display-name>
		<servlet-name>FarmerRequestListServlet</servlet-name>
		<servlet-class>edu.neu.agronauts.ui.servlets.FarmerRequestListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/home.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SaveFarmerRequestServlet</servlet-name>
		<url-pattern>/saveFarmerRequest.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ApproveServiceRequestServlet</servlet-name>
		<url-pattern>/approveServiceRequest.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FarmerRegisterServlet</servlet-name>
		<url-pattern>/farmerRegister.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FarmerRequestListServlet</servlet-name>
		<url-pattern>/farmerRequestList.html</url-pattern>
	</servlet-mapping>
